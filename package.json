{
    "name": "tsx-library-julseb",
    "private": false,
    "version": "6.2.16",
    "license": "MIT",
    "main": "./index.js",
    "exports": {
        ".": {
            "import": "./index.js",
            "require": "./index.js"
        },
        "./index.css": {
            "import": "./index.css",
            "require": "./index.css"
        },
        "./types": {
            "import": "./types.js",
            "require": "./types.js"
        }
    },
    "dependencies": {
        "@testing-library/jest-dom": "^5.14.1",
        "@testing-library/react": "^13.0.0",
        "@testing-library/user-event": "^13.2.1",
        "@uiw/react-md-editor": "^3.20.5",
        "fuse.js": "^6.6.2",
        "markdown-to-jsx": "^7.2.0",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-helmet": "^6.1.0",
        "react-inlinesvg": "^3.0.2",
        "react-linkify": "^1.0.0-alpha",
        "react-router-dom": "^6.9.0",
        "react-scripts": "5.0.1",
        "react-scroll-to-bottom": "^4.2.0",
        "react-syntax-highlighter": "^15.5.0",
        "styled-components": "^5.3.9",
        "ts-utils-julseb": "^2.2.10",
        "typescript": "^4.4.2",
        "web-vitals": "^2.1.0"
    },
    "scripts": {
        "start": "react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "clean": "rm -rf lib build && mkdir lib build",
        "add-git": "cd lib && git init && git remote add origin https://github.com/JulSeb42/tsx-library.git && git pull origin master && rm -rf * && rm -rf .gitignore && rm -rf .prettierrc",
        "copy-to-lib": "cp -a ./src/lib/* ./package.json ./.gitignore ./.prettierrc ./README.md ./lib",
        "copy-to-build": "cp ./package.json ./.gitignore ./.prettierrc ./README.md ./makefile ./lib/index.css ./lib/modules-declare.d.ts ./build",
        "copy-flags": "mkdir build/utils && cp -a lib/utils/flags build/utils/flags",
        "copy-icons": "mkdir build/icons-svg && cp lib/icons-svg/* build/icons-svg",
        "build-lib": "yarn clean && yarn add-git && yarn copy-to-lib && yarn copy-to-build && yarn copy-flags && yarn copy-icons && tsc",
        "publish-lib": "yarn publish ./build"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "@types/jest": "^29.5.0",
        "@types/node": "^18.15.5",
        "@types/react": "^18.0.28",
        "@types/react-dom": "^18.0.11",
        "@types/react-helmet": "^6.1.6",
        "@types/react-inlinesvg": "^1.0.0",
        "@types/react-linkify": "^1.0.1",
        "@types/react-syntax-highlighter": "^15.5.6",
        "@types/styled-components": "^5.1.26"
    }
}
